<?xml version="1.0" ?>

<robot  name="KneedCompassGait">
    <link name="base_link"/>
    <joint name="base" type="fixed">
        <origin xyz="0 0 0" rpy="1.5708 0 0"/>
        <child link="hip_line"/>
        <parent link="base_link"/>
    </joint>
    <link name="hip_line">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder length="0.4" radius="0.0075"/>
            </geometry>
            <material name="green">
                <color rgba="0 1 0 1"/>
            </material>
        </visual>
    </link>
    <link name="right_hip2roll"/>
    <link name="left_hip2roll"/>

    <link name="left_upper_leg">
        <visual>
            <origin xyz="0 0 -0.25" rpy="0 0 0"/>
            <geometry>
                <cylinder length="0.5" radius="0.0075"/>
            </geometry>
            <material name="red">
                <color rgba="1 0 0 1"/>
            </material>
        </visual>
    </link>
    <link name="left_upper_leg_mass">
        <visual>
            <origin xyz="0 0 -0.25"/>
            <geometry>
                <sphere radius="0.025"/>
            </geometry>
            <material name="red">
                <color rgba="1 0 0 1"/>
            </material>
        </visual>
        <inertial>
            <origin xyz="0 0 -0.25"/>
            <mass value="2.5"/>
            <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
        </inertial>
    </link>
    <joint name="left_upper_leg_mass_weld" type="fixed">
        <parent link="left_upper_leg"/>
        <child link="left_upper_leg_mass"/>
    </joint>
    <link name="left_lower_leg">
        <visual>
            <origin xyz="0 0 -0.25"/>
            <geometry>
                <cylinder length="0.5" radius="0.0075"/>
            </geometry>
            <material name="red">
                <color rgba="1 0 0 1"/>
            </material>
        </visual>
        <visual>
            <origin xyz="0 0 -0.5" />
            <geometry>
                <sphere radius="0.01"/>
            </geometry>
            <material name="red">
                <color rgba="1 0 0 1"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 -0.5"/>
            <geometry>
                <sphere radius="0"/>
            </geometry>
        </collision>
<!--        <visual>-->
<!--            <origin xyz="-0.01 0 -0.5" />-->
<!--            <geometry>-->
<!--                <sphere radius="0.01"/>-->
<!--            </geometry>-->
<!--            <material name="red">-->
<!--                <color rgba="1 0 0 1"/>-->
<!--            </material>-->
<!--        </visual>-->
